{
  "name": "google book Search",
  "version": "1.0.0",
  "description": "search for and save books you find using google's books api",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericjosprice/google-book-search.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ericjosprice/google-book-search/issues"
  },
  "homepage": "https://github.com/ericjosprice/google-book-search#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "http-server": "^0.11.1",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.5.5",
    "react-router-dom": "^5.0.0"
  }

}




